{
  "name": "MyElectronApp",
  "productName": "My Cool App",
  "version": "0.1.0",
  "description": "Electron app for macOS",
  "author": "Your Name",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "dev": "next dev --turbo -p 3301",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron": "electron electron.js",
    "build-mac": "electron-builder --mac"
  },
  "build": {
    "appId": "com.myapp.electron",
    "productName": "MyElectronApp",
    "files": [
      "electron-app.js",
      "electron/",
      "preload.js",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "assets/icon.icns",
      "hardenedRuntime": true,
      "entitlements": "electron/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "extendInfo": {
        "NSCameraUseContinuityCameraDeviceType": "YES"
      }
    }
  },
  "dependencies": {
    "@capacitor/cli": "^7.1.0",
    "@capacitor/core": "^7.1.0",
    "@capacitor/ios": "^7.1.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emailjs/browser": "^4.4.1",
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.7.2",
    "@supabase/supabase-js": "^2.47.10",
    "@tailwindcss/typography": "^0.5.16",
    "classnames": "^2.5.1",
    "cloudinary": "^2.5.1",
    "cloudinary-video-player": "^2.3.0",
    "dictionary-en": "^4.0.0",
    "dictionary-es": "^4.0.0",
    "dictionary-uk": "^4.0.0",
    "dompurify": "^3.2.3",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^11.16.1",
    "html-react-parser": "^5.2.2",
    "i18next": "^24.2.0",
    "lodash.debounce": "^4.0.8",
    "lorem-ipsum": "^2.0.8",
    "lucide-react": "^0.471.1",
    "mobile-detect": "^1.4.5",
    "next": "^15.5.7",
    "next-pwa": "^5.6.0",
    "nspell": "^2.1.5",
    "openai": "^4.85.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.1",
    "react-i18next": "^15.3.0",
    "react-icons": "^5.4.0",
    "react-range": "^1.10.0",
    "react-swipeable": "^7.0.2",
    "recorder-js": "^1.0.7",
    "styled-jsx": "^5.1.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "eruda": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
